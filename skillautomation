def fibonacci(n):
    fib_sequence = []
    a, b = 0, 1
    for _ in range(n):
        fib_sequence.append(a)
        a, b = b, a + b
    return fib_sequence

num_terms = 10  # Change this to the number of terms you want
fib_sequence = fibonacci(num_terms)

print(f"Fibonacci sequence up to {num_terms} terms:")
for term in fib_sequence:
    print(term, end=" ")

